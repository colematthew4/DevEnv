#!/bin/zsh

alias r='src'
alias refresh='src'
alias pshb='git push origin -u $(git rev-parse --abbrev-ref HEAD)'
alias gut='tug'
alias clint='commitlint --from HEAD~1 --verbose'
alias hex='printf "#%02X%02X%02X\n"'
alias rgb='{ read -r d && echo $d | sed "s/#//g" | sed "s/../0x&,/g" | awk -F "," "{ printf(\"%d %d %d\n\", \$1, \$2, \$3) }" } <<<'
